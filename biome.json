{
    "organizeImports": {
      "enabled": true
    },
    "formatter": {
      "enabled": true,
      "formatWithErrors": false,
      "ignore": [],
      "attributePosition": "auto",
      "indentStyle": "space",
      "indentWidth": 2,
      "lineWidth": 80,
      "lineEnding": "lf"
    },
    "javascript": {
      "formatter": {
        "arrowParentheses": "always",
        "bracketSameLine": true,
        "bracketSpacing": true,
        "jsxQuoteStyle": "double",
        "quoteProperties": "asNeeded",
        "semicolons": "always",
        "trailingCommas": "all"
      }
    },
    "json": {
      "formatter": {
        "trailingCommas": "all"
      }
    },
    "linter": {
      "enabled": false
    }
  }
  